  <head>
    <b:include data='blog' name='all-head-content'/>

    <!-- For code highlighting.
       @see http://code.google.com/p/syntaxhighlighter -->
    <link href='http://varzyfamily.com/links/blog/css/SyntaxHighlighter.css' rel='stylesheet' type='text/css'/>
    <script src='http://varzyfamily.com/links/blog/js/shCore.js' type='text/javascript'/>
    <script src='http://varzyfamily.com/links/blog/js/shBrushJScript.js' type='text/javascript'/>
    <script src='http://varzyfamily.com/links/blog/js/shBrushCss.js' type='text/javascript'/>
    <script src='http://varzyfamily.com/links/blog/js/shBrushRuby.js' type='text/javascript'/>
    <script src='http://varzyfamily.com/links/blog/js/shBrushJava.js' type='text/javascript'/>
<script src='http://varzyfamily.com/links/blog/js/shBrushXml.js' type='text/javascript'/>
<script src='http://varzyfamily.com/links/blog/js/shBrushSql.js' type='text/javascript'/>
<script src='http://varzyfamily.com/links/blog/js/shBrushPlain.js' type='text/javascript'/>
<script src='http://varzyfamily.com/links/blog/js/jquery-1.3.2.min.js' type='text/javascript'/>
